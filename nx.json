{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/.next"],
      "cache": true
    },
    "e2e": {
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "test": {
      "cache": true
    },
    "start": {
      "dependsOn": ["build"]
    }
  },
  "cachebust": "3",
  "defaultBase": "main",
  "nxCloudAccessToken": "NDA5MDNjZjctZWRmZS00NTA5LThhOWYtMjg4MWRmNDkzMjI2fHJlYWQtd3JpdGU=",
  "plugins": [
    {
      "plugin": "@nx/playwright/plugin",
      "options": {
        "targetName": "playwright:e2e"
      }
    }
  ]
}
